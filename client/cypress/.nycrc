{
  "reporter": ["lcov"],
  "include": ["app/js"],
  "exclude": ["cypress/**/*","tests/**/*", "node_modules/**/*", "app/lib/**/*", "build/**/*", "build/lib/js/locale/**/*"],
  "check-coverage": true,
  "per-file": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "report-dir": "cypress/coverage",
  "sourceMap": true
}